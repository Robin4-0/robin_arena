<?xml version="1.0"?>
<launch>
  <arg name="ur5_ns"         default="/ur5"    />
  <arg name="ur5_limited"    default="true"    />
  <arg name="gripper_enable" default="true"    />

  <group ns="$(arg ur5_ns)">

  <group if="$(arg gripper_enable)"> 
     <param   name="robot_description"
              command="$(find xacro)/xacro '$(find robin_arena)/urdf/manipulator_with_gripper_arena_setup.urdf.xacro' joint_limited:=$(arg ur5_limited)" />
  </group>

  <group unless="$(arg gripper_enable)"> 
     <param name="robot_description"
            command="$(find xacro)/xacro '$(find robin_arena)/urdf/manipulator_with_magnet_arena_setup.urdf.xacro' joint_limited:=$(arg ur5_limited)" />
  </group>
  
  </group>
</launch>
